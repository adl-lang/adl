module test5
{
   // Tests for unions

   union U1
   {
       Void v;
   };

   union U2
   {
       Int16 v;
   };

   union U3
   {
       Int16 v = 100;
   };

   struct S1
   {
      Int16 f = 100;
   };

   union U4
   {
       S1 v;
   };

   union U5
   {
       S1 v = { "f" : 200 };
   };

   union U6
   {
       U3 v;
   };

   union U7
   {
       U3 v = { "v" : 75 };
   };

   union U8
   {
       S1 v1;
       Int16 v2;
   };

   union U9<T>
   {
       T v1;
       Int16 v2;
       Void v3;
   };

   struct S {
       U9<String> f1 = {"v1":"xx"};
       U9<String> f2 = {"v2":100};
       U9<String> f3 = "v3";
   };

   // A recursive type

   union List<T>
   {
       Void null;
       Cell<T> cell;
   };

   struct Cell<T>
   {
        T head;
        List<T> tail;
   };
};
